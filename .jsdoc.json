{
  "source": {
    "include": ["src"],
    "includePattern": ".js$"
  },
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "template": "node_modules/clean-jsdoc-theme"
  },
  "markdown": {
      "hardwrap": false,
      "idInHeadings": true
  }
}